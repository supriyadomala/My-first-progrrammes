# My-first-progrrammes in C
# include<stdio.h>
int main() {
printf("Welcome to C language");
# Even-odd numbers
printf("Enter a value : \n");
scanf("%d", &a);
if(a%2==0) 
printf("%d is an Even number",a);
else
printf("%d is an Odd number",a);
# Swaping 2 numbers
printf("Enter m,n values\n");
scanf("%d%d",&m,&n);
printf(" Before swaping m=%d, n=%d",&m,&n);
a=a^b;  // a=a+b        a=a*b
b=a^b;  // b=a-b  (or)  b=a//b
a=a^b;  // a=a-b        a=a//b
printf(" After swaping m=%d, n=%d ", &m, &n);
